{
  "expo": {
    "name": "E-Learning AI App",
    "slug": "e-learning-ai-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elearning.ai.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.elearning.ai.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "elearning-ai-app",
    "platforms": ["ios", "android", "web"]
  }
} 